{
  "name": "quiz-oyunu",
  "version": "14.26.0",
  "description": "Bilgoo uygulaması, kullanıcıların bilgilerini çeşitli kategorilerde test edebilecekleri interaktif bir web uygulamasıdır.",
  "main": "achievements.js",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sync:all": "./sync-files.sh",
    "sync:files": "cp index.html style.css script.js *.js *.css *.html *.json www/ 2>/dev/null || true",
    "build:ios": "npm run sync:files && npx cap sync ios && npx cap open ios",
    "sync:ios": "npx cap sync ios",
    "open:ios": "npx cap open ios",
    "build:android": "npm run sync:files && npx cap sync android && npx cap open android",
    "sync:android": "npx cap sync android",
    "open:android": "npx cap open android",
    "copy:www": "cp -r *.html *.css *.js *.json *.ico *.svg *.png *.md icons/ languages/ www/ 2>/dev/null || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@capacitor-community/admob": "^7.0.3",
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/push-notifications": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "capacitor-plugin-app-tracking-transparency": "^2.0.5",
    "firebase": "^12.7.0",
    "next": "^16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "vite": "^7.3.0"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veteroner/bilgoo.git"
  },
